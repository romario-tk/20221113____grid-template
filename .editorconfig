root = true

[*]
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2
end_of_line = lf
max_line_length=150

[*.md]
trim_trailing_whitespace = false

[/node_modules/**, '/libs/**', '/vendor/**']
charset = unset
end_of_line = unset
insert_final_newline = unset
trim_trailing_whitespace = unset
indent_style = unset
indent_size = unset
